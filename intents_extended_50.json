    },
    {
      "tag": "change_preferences",
      "patterns": [
        "Change my filters",
        "Edit my search"
      ],
      "responses": [
        "No problem. What would you like to update?"
      ]
    },
    {
      "tag": "feedback",
      "patterns": [
        "This was helpful",
        "You\u2019re great"
      ],
      "responses": [
        "Thank you! I'm glad I could help."
      ]
    },
    {
      "tag": "not_helpful",
      "patterns": [
        "This is useless",
        "Didn't help"
      ],
      "responses": [
        "I\u2019m sorry about that. Let me try another way."
      ]
    }
  ]
}
